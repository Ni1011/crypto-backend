{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "source": "/api/users/?(.*)",
      "dest": "/routes/userRoutes"
    },
    {
      "source": "/api/income/?(.*)",
      "dest": "/routes/incomeRoute"
    },
    {
      "source": "/api/expense/?(.*)",
      "dest": "/routes/expenseRoute"
    }
  ]
}
