{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users(.*)",
      "dest": "/userRoute.js"
    },
    {
      "src": "/api/income(.*)",
      "dest": "/incomeRoute.js"
    },
    {
      "src": "/api/expense(.*)",
      "dest": "/expenseRoute.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
