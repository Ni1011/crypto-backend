{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/?(.*)",
      "dest": "/routes/userRoutes.js"
    },
    {
      "src": "/api/income/?(.*)",
      "dest": "/routes/incomeRoute.js"
    },
    {
      "src": "/api/expense/?(.*)",
      "dest": "/routes/expenseRoute.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
